{
  "schemaVersion": 1,
  "id": "createdeco",
  "version": "${version}",
  "name": "Create Deco",
  "description": "Decorative options for your Create factory",
  "authors": [
    "Kayla, Talrey"
  ],
  "license": "Insert License Here",
  "icon": "assets/createdeco/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.talrey.createdeco.fabric.CreateDecoModFabric"
    ],
    "fabric-datagen": [
      "com.github.talrey.createdeco.fabric.CreateDecoModData"
    ]
  },
  "mixins": [
    "createdeco.mixins.json",
    "createdeco-common.mixins.json"
  ],
  "depends": {
    "minecraft": "${minecraft_version}",
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}"
  }
}
